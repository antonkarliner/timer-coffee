{
    "builds": [
        {
            "src": "/flutter.git",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build/web"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "credentialless"
                }
            ]
        }
    ]
}
{
    "rewrites":  [
      {"source": "/(.*)", "destination": "/"}
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "credentialless"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://timercoffeeapp.fra1.cdn.digitaloceanspaces.com/"
          }
        ]
      }
    ]
  }